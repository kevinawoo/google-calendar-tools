{
  "name": "google-calendar-tools",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "20.18.0"
  },
  "scripts": {
    "lint": "eslint --fix src/**/*.ts",
    "test": "jest",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build": "npm run lint && npm run test && rimraf dist && webpack && cpx src/appsscript.json dist && cpx \"src/**/*.html\" dist",
    "build-push": "npm run build && clasp push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinawoo/google-calendar-tools.git"
  },
  "keywords": [
    "google-calendar",
    "calendar",
    "google-apps-script",
    "clasp"
  ],
  "author": "kevinawoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinawoo/google-calendar-tools/issues"
  },
  "homepage": "https://github.com/kevinawoo/google-calendar-tools#readme",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/google-apps-script": "^1.0.84",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "cpx": "^1.5.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gas-webpack-plugin": "^2.6.0",
    "jest": "^29.7.0",
    "moment": "^2.30.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sunrise-sunset-js": "^2.2.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/.+(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
