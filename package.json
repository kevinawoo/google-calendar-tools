    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@google/clasp": "^2.4.0",
    "moment": "^2.29.2",
    "sunrise-sunset-js": "^2.1.2"
